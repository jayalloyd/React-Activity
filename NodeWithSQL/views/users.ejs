<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <style>
        table {
            border-collapse: collapse;
            width: 100%;
        }
        th, td {
            border: 1px solid black;
            padding: 8px;
            text-align: left;
        }
        th {
            background-color: #f2f2f2;
        }
        </style>
</head>
<body>
   <table>
       <thead>
           <tr>
               <th>id</th>
               <th>name</th>
               <th>email</th>
           </tr>
       </thead>
       <tbody>
           <% users.forEach(user => { %>
               <tr>
                   <td><%= user.id %></td>
                   <td><%= user.name %></td>
                   <td><%= user.email %></td>
               </tr>
           <% }) %>
       </tbody>
    </table>
</body>
</html>